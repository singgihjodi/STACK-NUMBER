class stack
{
	public :
	int head;
	data[MAX];
}
tmp;
void init(){
	tmp.head = -1;
}

bool isEmpty() {
  return tmp.head == -1;
}

bool isFull() {
	return tmp.head == MAX-1;
}
void push() {
   if (isFull()) {
		cout << "\nTumpukan penuh"<<endl;
	}
	else {
   		tmp.head++;
    		cout << "\nMasukkan data = "; cin >> tmp.data[tmp.head];
    				cout << "Data " << tmp.data[tmp.head] << " masuk ke stack"<<endl;
	}
}


